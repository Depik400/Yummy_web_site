<div class="content__page">
  <div class="container">
    <div class="content__page__all">
      {{#each titles}}
      <div class="content__page__movie">

        <div class="title_id">
          <h3>{{title}}</h3>
        </div>
        <div class="titles_main_info">
          <h3>{{rating}}</h3>
          <h3>{{type}}</h3>
          <h3>{{series}}</h3>
          <h3></h3>
        </div>
        <div class="box">
          <img src="{{image_path}}" alt="{{title}}">
        </div>

      </div>
      {{/each}}
    </div>
  </div>
</div>